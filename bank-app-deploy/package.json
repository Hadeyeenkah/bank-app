{
  "name": "aurora-bank",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:5001",
  "scripts": {
    "start": "concurrently \"npm --prefix ../backend start\" \"react-scripts start\"",
    "dev": "react-scripts start",
    "dev:api": "node dev-server.js",
    "dev:all": "concurrently \"node dev-server.js\" \"npm start\"",
    "dev:start": "lsof -t -i:4000 | xargs -r kill -9 || true && lsof -t -i:5001 | xargs -r kill -9 || true && lsof -t -i:3000 | xargs -r kill -9 || true && BACKEND_URL=http://localhost:5001 npm run dev:all",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preview": "react-scripts build && serve -s build",
    "seed:reset": "node ../backend/scripts/reset_admin_password.js || true"
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "nodemailer": "^7.0.12",
    "qrcode": "^1.5.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^6.30.2",
    "speakeasy": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.0",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "postinstall-postinstall": "^2.1.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }

{
  "version": 2,
  "builds": [
    {
      "src": "bank-app-deploy/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "bank-app-deploy/build"
      }
    },
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "bank-app-deploy/build/index.html"
    }
  ]
}
